#include<iostream>
using namespace std;

int reverseArray(int n, int arr[])
{
    
    for(int i=n-1;i>=0;i--)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
int main(){
    int n;
    cin>>n;
    int arr[n];

    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    reverseArray(n,arr);
    return 0;
}
